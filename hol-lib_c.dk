             (exp_add_rule
                b
                n
                (mult n m)
                (w:(El nat) => eq nat (exp b (mult n (S m))) w)
                (add_comm_rule
                   (mult n m)
                   n
                   (w:(El nat) => eq nat (exp b (mult n (S m))) (exp b w))
                   (eq_refl_rule nat (exp b (mult n (S m))))))))).

