thm exp_mult :
  Prf
    (not
       (not
          (all
             nat
             (b:(El nat) =>
              not
                (not
                   (all
                      nat
                      (n:(El nat) =>
                       not
                         (not
                            (all
                               nat
                               (m:(El nat) =>
                                not (not (eq nat (exp b (mult n m)) (exp (exp b n) m)))))))))))))
  :=
  all_i_i
    nat
    (b:(El nat) =>
     all
       nat
       (n:(El nat) =>
        not (not (all nat (m:(El nat) => not (not (eq nat (exp b (mult n m)) (exp (exp b n) m))))))))
    (b:(El nat) =>
     all_i_i
       nat
       (n:(El nat) =>
        all nat (m:(El nat) => not (not (eq nat (exp b (mult n m)) (exp (exp b n) m)))))
       (n:(El nat) =>
