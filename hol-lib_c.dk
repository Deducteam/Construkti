def leq : (El nat) -> (El nat) -> Prop.

[_1, _0] leq _0 _1 --> ex nat (p:(El nat) => eq nat (add _0 p) _1).

thm leq_refl :
  n:(El nat) -> Prf (not (not (leq n n)))
  :=
  n:(El nat) => ex_i_i nat (p:(El nat) => eq nat (add n p) n) 0 (eq_refl_rule nat n).

thm leq_trans :
  n:(El nat) ->
  m:(El nat) ->
  p:(El nat) ->
  (Prf (not (not (leq n m)))) -> (Prf (not (not (leq m p)))) -> Prf (not (not (leq n p)))
  :=
  n:(El nat) =>
  m:(El nat) =>
  p:(El nat) =>
  Pnm:(Prf (not (not (leq n m)))) =>
  Pmp:(Prf (not (not (leq m p)))) =>
  ex_e_i
    nat
    (x:(El nat) => eq nat (add n x) m)
    Pnm
    (leq n p)
    (x:(El nat) =>
     Px:(Prf (not (not (eq nat (add n x) m)))) =>
     ex_e_i
       nat
       (y:(El nat) => eq nat (add m y) p)
       Pmp
       (leq n p)
       (y:(El nat) =>
        Py:(Prf (not (not (eq nat (add m y) p)))) =>
        ex_i_i
          nat
          (z:(El nat) => eq nat (add n z) p)
          (add x y)
          (Py
             (w:(El nat) => eq nat (add n (add x y)) w)
             (Px
                (w:(El nat) => eq nat (add n (add x y)) (add w y))
                (eq_sym_rule nat (add (add n x) y) (add n (add x y)) (add_assoc_rule n x y)))))).

thm leq_antisym :
  n:(El nat) ->
  m:(El nat) ->
  (Prf (not (not (leq n m)))) -> (Prf (not (not (leq m n)))) -> Prf (not (not (eq nat n m)))
  :=
  n:(El nat) =>
  m:(El nat) =>
  Pnm:(Prf (not (not (leq n m)))) =>
  Pmn:(Prf (not (not (leq m n)))) =>
  ex_e_i
    nat
    (x:(El nat) => eq nat (add n x) m)
    Pnm
    (eq nat n m)
    (x:(El nat) =>
     Px:(Prf (not (not (eq nat (add n x) m)))) =>
     ex_e_i
       nat
       (y:(El nat) => eq nat (add m y) n)
       Pmn
       (eq nat n m)
       (y:(El nat) =>
        Py:(Prf (not (not (eq nat (add m y) n)))) =>
        imp_e_i
          (eq nat x 0)
          (eq nat n m)
          (imp_i_i
             (eq nat x 0)
             (eq nat n m)
             (PxZ:(Prf (not (not (eq nat x 0)))) => PxZ (w:(El nat) => eq nat (add n w) m) Px))
          (imp_e_i
             (eq nat (add x y) 0)
             (eq nat x 0)
             (all_e_i
                nat
                (m1:(El nat) => imp (eq nat (add x m1) 0) (eq nat x 0))
                (all_e_i
                   nat
                   (n1:(El nat) =>
                    all
                      nat
                      (m1:(El nat) => not (not (imp (eq nat (add n1 m1) 0) (eq nat n1 0)))))
                   add_left_cancel_Z
                   x)
                y)
             (add_left_cancel_rule
                (add x y)
                n
                (add_assoc_rule
                   n
                   x
                   y
                   (w:(El nat) => eq nat w n)
                   (eq_sym_rule nat (add n x) m Px (w:(El nat) => eq nat (add w y) n) Py)))))).

