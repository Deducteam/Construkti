def exp : (El nat) -> (El nat) -> El nat.

[b] exp b (0) --> S 0.

[x, b] exp b (S x) --> mult b (exp b x).

thm mult_one :
  Prf (not (not (all nat (n:(El nat) => not (not (eq nat n (mult n (S 0))))))))
  :=
  all_i_i
    nat
    (n:(El nat) => eq nat n (mult n (S 0)))
    (n:(El nat) =>
     all_e_i
       nat
       (n1:(El nat) => eq nat (add 0 n1) n1)
       add_Zn
       n
       (w:(El nat) => eq nat w (add 0 n))
       (eq_refl_rule nat (add 0 n))).

thm exp_add :
  Prf
    (not
       (not
          (all
             nat
             (b:(El nat) =>
              not
                (not
                   (all
                      nat
                      (n:(El nat) =>
                       not
                         (not
                            (all
                               nat
                               (m:(El nat) =>
                                not
                                  (not
                                     (eq nat (exp b (add n m)) (mult (exp b n) (exp b m))))))))))))))
  :=
  all_i_i
    nat
    (b:(El nat) =>
     all
       nat
       (n:(El nat) =>
        not
          (not
             (all
                nat
                (m:(El nat) => not (not (eq nat (exp b (add n m)) (mult (exp b n) (exp b m)))))))))
    (b:(El nat) =>
     all_i_i
       nat
       (n:(El nat) =>
        all nat (m:(El nat) => not (not (eq nat (exp b (add n m)) (mult (exp b n) (exp b m))))))
       (n:(El nat) =>
        induction_rule
          (m:(El nat) => eq nat (exp b (add n m)) (mult (exp b n) (exp b m)))
          (all_e_i
             nat
             (n1:(El nat) => eq nat n1 (mult n1 (S 0)))
             mult_one
             (exp b n)
             (w:(El nat) => eq nat (exp b (add n 0)) w)
             (eq_refl_rule nat (exp b n)))
          (m:(El nat) =>
           pPm:(Prf (not (not (eq nat (exp b (add n m)) (mult (exp b n) (exp b m)))))) =>
           mult_assoc_rule
             (exp b n)
             b
             (exp b m)
             (w:(El nat) => eq nat (mult b (exp b (add n m))) w)
             (mult_comm_rule
                b
                (exp b n)
                (w:(El nat) => eq nat (mult b (exp b (add n m))) (mult w (exp b m)))
                (eq_sym_rule
                   nat
                   (exp b (add n m))
                   (mult (exp b n) (exp b m))
                   pPm
                   (w:(El nat) => eq nat (mult b w) (mult (mult b (exp b n)) (exp b m)))
                   (mult_assoc_rule
                      b
                      (exp b n)
                      (exp b m)
                      (w:(El nat) => eq nat w (mult (mult b (exp b n)) (exp b m)))
                      (eq_refl_rule nat (mult (mult b (exp b n)) (exp b m))))))))).

thm exp_add_rule :
  b:(El nat) ->
  n:(El nat) -> m:(El nat) -> Prf (not (not (eq nat (exp b (add n m)) (mult (exp b n) (exp b m)))))
  :=
